{
  "name": "react-snips",
  "displayName": "react snips",
  "description": "A react snippet extension that conforms to eslint definitions of react functional components",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript,javascriptreact,typescript,typescriptreact",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  }
}
